	<div class="user_stuff">
  <% unless current_user %>
      <%= link_to t(:login), login_url, :class=>'btn btn-success' %>
      <%= link_to t('users.new'), register_url,:class=>'btn btn-info' %>
  <% else %>
      <%= user_label_small(current_user) %>
      
     <%= link_to t(:profile),profile_url,:class=>'btn btn-success'%>
      
      <%= link_to t(:logout),logout_url, :class=>'btn btn-danger'%>
  <% end %>
</div>